## arrow up
"\e[A":history-search-backward
## arrow down
"\e[B":history-search-forward

# set editing-mode vi
# set keymap vi

"\e[5C": forward-word
"\e[5D": backward-word
"\e[1;5C": forward-word
"\e[1;5D": backward-word
"\e[C": forward-word
"\e[D": backward-word
"\eOC": forward-word
"\eOD": backward-word

set completion-ignore-case on
